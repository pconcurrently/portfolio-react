{
  "name": "react-typescript-starter",
  "version": "1.1.0",
  "description": "React starter with Typescript",
  "main": "src/index.tsx",
  "subdirectory": "",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "redux",
    "redux-thunk",
    "sass",
